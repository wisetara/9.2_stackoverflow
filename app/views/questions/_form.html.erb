<%= render("layouts/errors", :object => @question) %>

<%= form_for @question, :html => { :multipart => true } do |f| %>

  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div>

  <%= f.submit(:class => "btn btn-primary") %>
<% end %>
